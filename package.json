        "menus": {
            "touchBar": [
                {
                    "command": "workbench.action.files.saveAll",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.saveAll && !enabledGroup"
                },
                {
                    "command": "editor.toggleFold",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.fold && !enabledGroup"
                },
                {
                    "command": "nasc.touchBar.installScript",
                    "group": "terminalStuff",
                    "when": "config.nasc-touchbar.installScript && !enabledGroup"
                },
                {
                    "command": "nasc.touchBar.serveScript",
                    "group": "terminalStuff",
                    "when": "config.nasc-touchbar.serveScript && !enabledGroup"
                },
                {
                    "command": "nasc.touchBar.buildScript",
                    "group": "terminalStuff",
                    "when": "config.nasc-touchbar.buildScript && !enabledGroup"
                },
                {
                    "command": "nasc.touchBar.lintScript",
                    "group": "terminalStuff",
                    "when": "config.nasc-touchbar.lintScript && !enabledGroup"
                },
                {
                    "command": "nasc.touchBar.stopTerminal",
                    "group": "terminalStuff",
                    "when": "config.nasc-touchbar.stopTerminal && !enabledGroup"
                },
                {
                    "command": "nasc.touchBar.refreshTerminal",
                    "group": "terminalStuff",
                    "when": "config.nasc-touchbar.refreshTerminal && !enabledGroup"
                },
                {
                    "command": "nasc.touchBar.enableFuncGroup",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.enableFuncGroup && !enabledGroup"
                },
                {
                    "command": "nasc.touchBar.enableSrcGroup",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.enableSrcGroup && !enabledGroup"
                },
                {
                    "command": "nasc.touchBar.enableCursorsGroup",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.enableCursorsGroup && !enabledGroup"
                },
                {
                    "command": "nasc.touchBar.enableEditorGroup",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.enableEditorGroup && !enabledGroup"
                },
                {
                    "command": "nasc.touchBar.closeGroup",
                    "group": "nasc",
                    "when": "enabledGroup"
                },
                {
                    "command": "nasc.touchBar.goToDefinition",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.goToDefinition && !enabledGroup"
                },
                {
					"command": "editor.action.formatDocument",
					"group": "nasc",
					"when": "config.nasc-touchbar.formatDocument && !enabledGroup"
				},
                {
                    "command": "editor.action.rename",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.rename && !enabledGroup"
                },
                {
                    "command": "editor.action.rename",
                    "group": "nasc",
                    "when": "enabledFuncGroup"
                },
                {
                    "command": "editor.action.marker.nextInFiles",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.nextInFiles"
                },
                {
                    "command": "editor.action.previewDeclaration",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.peekDefinition && !enabledGroup"
                },
                {
                    "command": "editor.action.previewDeclaration",
                    "group": "nasc",
                    "when": "enabledFuncGroup"
                },
                {
                    "command": "editor.action.referenceSearch.trigger",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.showReferences && !enabledGroup"
                },
                {
                    "command": "editor.action.referenceSearch.trigger",
                    "group": "nasc",
                    "when": "enabledFuncGroup"
                },
                {
                    "command": "editor.action.jumpToBracket",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.jumpToBracket && !enabledGroup"
                },
                {
                    "command": "editor.action.jumpToBracket",
                    "group": "nasc",
                    "when": "enabledFuncGroup"
                },
                {
                    "command": "nasc.touchBar.addCursorAbove",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.addCursorAbove"
                },
                {
                    "command": "nasc.touchBar.addCursorAbove",
                    "group": "nasc",
                    "when": "enabledCursorsGroup"
                },
                {
                    "command": "nasc.touchBar.addCursorBelow",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.addCursorBelow && !enabledGroup"
                },
                {
                    "command": "nasc.touchBar.addCursorBelow",
                    "group": "nasc",
                    "when": "enabledCursorsGroup"
                },
                {
                    "command": "editor.action.addSelectionToNextFindMatch",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.selectNext && !enabledGroup"
                },
                {
                    "command": "editor.action.addSelectionToNextFindMatch",
                    "group": "nasc",
                    "when": "enabledCursorsGroup"
                },
                {
                    "command": "editor.action.moveSelectionToNextFindMatch",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.goToNext && !enabledGroup"
                },
                {
                    "command": "editor.action.moveSelectionToNextFindMatch",
                    "group": "nasc",
                    "when": "enabledCursorsGroup"
                },
                {
                    "command": "workbench.action.toggleSidebarVisibility",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.toggleSidebar && !enabledGroup"
                },
                {
                    "command": "workbench.action.toggleSidebarVisibility",
                    "group": "nasc",
                    "when": "enabledEditorGroup"
                },
                {
                    "command": "workbench.action.togglePanel",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.togglePanel && !enabledGroup"
                },
                {
                    "command": "workbench.action.togglePanel",
                    "group": "nasc",
                    "when": "enabledEditorGroup"
                },
                {
                    "command": "workbench.action.showCommands",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.showCommands && !enabledGroup"
                },
                {
                    "command": "workbench.action.showCommands",
                    "group": "nasc",
                    "when": "enabledEditorGroup"
                },
                {
                    "command": "editor.action.toggleRenderWhitespace",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.toggleWhiteSpace && !enabledGroup"
                },
                {
                    "command": "editor.action.toggleRenderWhitespace",
                    "group": "nasc",
                    "when": "enabledEditorGroup"
                },
                {
                    "command": "workbench.action.openGlobalSettings",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.settings && !enabledGroup"
                },
                {
                    "command": "editor.action.copyLinesDownAction",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.copyLineDown && !enabledGroup"
                },
                {
                    "command": "editor.action.copyLinesDownAction",
                    "group": "nasc",
                    "when": "enabledSrcGroup"
                },
                {
                    "command": "editor.action.indentLines",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.indent && !enabledGroup"
                },
                {
                    "command": "editor.action.indentLines",
                    "group": "nasc",
                    "when": "enabledSrcGroup"
                },
                {
                    "command": "editor.action.outdentLines",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.outdent && !enabledGroup"
                },
                {
                    "command": "editor.action.outdentLines",
                    "group": "nasc",
                    "when": "enabledSrcGroup"
                },
                {
                    "command": "editor.action.blockComment",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.blockComment && !enabledGroup"
                },
                {
                    "command": "editor.action.blockComment",
                    "group": "nasc",
                    "when": "enabledSrcGroup"
                },
                {
                    "command": "editor.action.commentLine",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.commentLine && !enabledGroup"
                },
                {
                    "command": "git.sync",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.gitSync && !enabledGroup"
                },
                {
                    "command": "extension.dash.emptySyntax",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.docs && !editorHasSelection && !enabledGroup || config.nasc-touchbar.docs && !config.nasc-touchbar.docsSelection && !enabledGroup"
                },
                {
                    "command": "extension.dash.specific",
                    "group": "nasc",
                    "when": "config.nasc-touchbar.docsSelection && editorHasSelection && !enabledGroup || config.nasc-touchbar.docsSelection && !config.nasc-touchbar.docs && !enabledGroup"
                }
            ]
        },
        "configuration": [
            {
                "title": "Nasc Touch Bar Settings",
                "type": "object",
                "properties": {
                    "nasc-touchbar.saveAll": {
                        "type": "boolean",
                        "default": false,
                        "description": "Save all files"
                    },
                    "nasc-touchbar.fold": {
                        "type": "boolean",
                        "default": false,
                        "description": "Fold / Unfold current scope"
                    },
                    "nasc-touchbar.installScript": {
                        "type": "boolean",
                        "default": false,
                        "description": "Install current projects devDependencies using NPM"
                    },
                    "nasc-touchbar.serveScript": {
                        "type": "boolean",
                        "default": false,
                        "description": "Serve current project using NPM"
                    },
                    "nasc-touchbar.buildScript": {
                        "type": "boolean",
                        "default": false,
                        "description": "Build current project using NPM"
                    },
                    "nasc-touchbar.lintScript": {
                        "type": "boolean",
                        "default": false,
                        "description": "Lint current project using NPM"
                    },
                    "nasc-touchbar.stopTerminal": {
                        "type": "boolean",
                        "default": false,
                        "description": "Send ctrl + c to terminal"
                    },
                    "nasc-touchbar.refreshTerminal": {
                        "type": "boolean",
                        "default": false,
                        "description": "Send ctrl + c to terminal and rerun last command (up arrow)"
                    },
                    "nasc-touchbar.goToDefinition": {
                        "type": "boolean",
                        "default": true,
                        "description": "Go to Definition"
                    },
                    "nasc-touchbar.addCursorBelow": {
                        "type": "boolean",
                        "default": true,
                        "description": "Add cursor below"
                    },
                    "nasc-touchbar.formatDocument": {
						"type": "boolean",
						"default": true,
						"description": "Format the document"
					},
                    "nasc-touchbar.rename": {
                        "type": "boolean",
                        "default": true,
                        "description": "Rename Symbol. It will replace the variable or function name (for example) everywhere"
                    },
                    "nasc-touchbar.nextInFiles": {
                        "type": "boolean",
                        "default": false,
                        "description": "Jump to next error in project"
                    },
                    "nasc-touchbar.togglePanel": {
                        "type": "boolean",
                        "default": true,
                        "description": "Toggle Panel"
                    },
                    "nasc-touchbar.showCommands": {
                        "type": "boolean",
                        "default": true,
                        "description": "Show Commands"
                    },
                    "nasc-touchbar.selectNext": {
                        "type": "boolean",
                        "default": false,
                        "description": "Select the next matching string (if a word is selected, uses this to search)"
                    },
                    "nasc-touchbar.enableFuncGroup": {
                        "type": "boolean",
                        "default": false,
                        "description": "Enables the Function group"
                    },
                    "nasc-touchbar.enableSrcGroup": {
                        "type": "boolean",
                        "default": false,
                        "description": "Enables the Source group"
                    },
                    "nasc-touchbar.enableCursorsGroup": {
                        "type": "boolean",
                        "default": false,
                        "description": "Enables the Cursors group"
                    },
                    "nasc-touchbar.enableEditorGroup": {
                        "type": "boolean",
                        "default": false,
                        "description": "Enables the Editor group"
                    },
                    "nasc-touchbar.toggleSidebar": {
                        "type": "boolean",
                        "default": false,
                        "description": "Toggle Sidebar"
                    },
                    "nasc-touchbar.addCursorAbove": {
                        "type": "boolean",
                        "default": false,
                        "description": "Add cursor above"
                    },
                    "nasc-touchbar.copyLineDown": {
                        "type": "boolean",
                        "default": false,
                        "description": "Copy the current line below"
                    },
                    "nasc-touchbar.goToNext": {
                        "type": "boolean",
                        "default": false,
                        "description": "Go to the next match (if a word is selected, uses this to search)"
                    },
                    "nasc-touchbar.toggleWhiteSpace": {
                        "type": "boolean",
                        "default": false,
                        "description": "Show or hide white spaces"
                    },
                    "nasc-touchbar.peekDefinition": {
                        "type": "boolean",
                        "default": false,
                        "description": "Peek definition/declaration"
                    },
                    "nasc-touchbar.showReferences": {
                        "type": "boolean",
                        "default": false,
                        "description": "List references who use the current symbol"
                    },
                    "nasc-touchbar.settings": {
                        "type": "boolean",
                        "default": false,
                        "description": "Open user settings"
                    },
                    "nasc-touchbar.indent": {
                        "type": "boolean",
                        "default": false,
                        "description": "Indent text"
                    },
                    "nasc-touchbar.outdent": {
                        "type": "boolean",
                        "default": false,
                        "description": "Outdent text"
                    },
                    "nasc-touchbar.jumpToBracket": {
                        "type": "boolean",
                        "default": false,
                        "description": "Jump to matching bracket"
                    },
                    "nasc-touchbar.blockComment": {
                        "type": "boolean",
                        "default": false,
                        "description": "Toggles block comments ( /* ... */ ) for the current selection"
                    },
                    "nasc-touchbar.commentLine": {
                        "type": "boolean",
                        "default": false,
                        "description": "Toggles line comments ( // ) for the current selection"
                    },
                    "nasc-touchbar.gitSync": {
                        "type": "boolean",
                        "default": false,
                        "description": "Git Sync"
                    },
                    "nasc-touchbar.docs": {
                        "type": "boolean",
                        "default": false,
                        "description": "Documentation"
                    },
                    "nasc-touchbar.docsSelection": {
                        "type": "boolean",
                        "default": false,
                        "description": "Opens documentation for current selection"
                    }
                }
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test",
        "deploy": "vsce publish minor",
        "deploy-fix": "vsce publish patch",
        "deploy-upgrade": "vsce publish major"
    },
    "license": "MIT",
    "icon": "nasc-macbook-pro-vscode-touch-bar.jpg",
    "devDependencies": {
        "typescript": "^2.5.2",
        "vscode": "^1.1.5",
        "mocha": "^3.5.0",
        "eslint": "^4.6.1",
        "@types/node": "^7.0.0",
        "@types/mocha": "^2.2.42"
    }
}
